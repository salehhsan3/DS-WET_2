    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            // "args": [],
            // "args": [ "<" , "${workspaceFolder}/test.in.txt" ],
            // "args": [ "<" , "${workspaceFolder}/Tests/WithOutBonus/input/in0.txt" ],
            "args": [ "<" , "${workspaceFolder}/Tests/input/in0.txt" ],
            // "args": [ "<" , "${workspaceFolder}/Tests/WithOutBonus/input/in0.txt" ,
            //           ">" , "${workspaceFolder}/Tests/WithOutBonus/myoutput/my_out0.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment" : [],
            "externalConsole" : false,    
            "console": "integratedTerminal"
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}